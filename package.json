    "chai": "^3.5.0",
    "chai-datetime": "^1.4.1",
    "mocha": "^3.4.1",
    "rimraf": "^2.6.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.3"
  },
  "dependencies": {
    "@types/node-fetch": "^1.6.7",
